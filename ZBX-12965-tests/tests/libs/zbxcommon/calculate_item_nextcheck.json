[
	{
		"test_case": "happy path",
		"in": {
			"names": ["TZ", "interval", "seed", "type", "now"],
			"values": ["UTC", "1m", 33, 0, "1970-01-01 03:00:00+03:00"]
		},
		"out": {
			"names": ["return", "error", "nextcheck_num", "nextcheck#1", "nextcheck#2", "nextcheck#3"],
			"values": [0, "N/A", 3, "1970-01-01 03:00:33+03:00", "1970-01-01 03:01:33+03:00", "1970-01-01 03:02:33+03:00"]
		}
	},
	{
		"test_case": "scheduling interval with month day and week day filters (next check should be on Saturday August 1st 1970)",
		"in": {
			"names": ["TZ", "interval", "seed", "type", "now"],
			"values": ["UTC", "0;md1wd6", 0, 0, "1970-01-01 03:00:00+03:00"]
		},
		"out": {
			"names": ["return", "error", "nextcheck_num", "nextcheck#1"],
			"values": [0, "N/A", 1, "1970-08-01 03:00:00+03:00"]
		}
	},
	{
		"test_case": "invalid scheduling interval",
		"in": {
			"names": ["TZ", "interval", "seed", "type", "now"],
			"values": ["UTC", "0;foobar", 0, 0, "1970-01-01 03:00:00+03:00"]
		},
		"out": {
			"names": ["return", "error", "nextcheck_num"],
			"values": [-1, "Invalid scheduling interval \"foobar\".", 0]
		}
	},
	{
		"test_case": "short scheduling interval and DST transition (Sun Oct 29 03:59:00 EEST 2017, Sun Oct 29 03:00:00 EET 2017, Sun Oct 29 03:01:00 EET 2017, ...)",
		"in": {
			"names": ["TZ", "interval", "seed", "type", "now"],
			"values": [":Europe/Riga", "0;m0-59", 0, 0, "2017-10-29 03:59:00+03:00"]
		},
		"out": {
			"names": ["return", "error", "nextcheck_num", "nextcheck#1", "nextcheck#2"],
			"values": [0, "N/A", 2, "2017-10-29 04:00:00+03:00", "2017-10-29 04:01:00+03:00"]
		}
	},
	{
		"test_case": "long scheduling interval and DST transition (Sun Oct 29 02:00:00 EEST 2017, Mon Oct 30 02:00:00 EET 2017, Tue Oct 31 02:00:00 EET 2017, ...)",
		"in": {
			"names": ["TZ", "interval", "seed", "type", "now"],
			"values": [":Europe/Riga", "0;h2", 0, 0, "2017-10-29 02:00:00+03:00"]
		},
		"out": {
			"names": ["return", "error", "nextcheck_num", "nextcheck#1", "nextcheck#2"],
			"values": [0, "N/A", 2, "2017-10-30 03:00:00+03:00", "2017-10-31 03:00:00+03:00"]
		}
	},
	{
		"test_case": "one of scheduling interval month days is outside of current month day range (Mon Feb  1 00:00:00 UTC 1970, Sun Mar  1 00:00:00 UTC 1970)",
		"in": {
			"names": ["TZ", "interval", "seed", "type", "now"],
			"values": ["UTC", "0;md1,31", 0, 0, "1970-02-01 03:00:00+03:00"]
		},
		"out": {
			"names": ["return", "error", "nextcheck_num", "nextcheck#1"],
			"values": [0, "N/A", 1, "1970-03-01 03:00:00+03:00"]
